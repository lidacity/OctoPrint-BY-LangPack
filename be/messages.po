msgid "Logout"
msgstr "Выйсці"

#: src/octoprint/templates/navbar/offlineindicator.jinja2:1
msgid "There is no connection to the internet"
msgstr "Няма злучэння з інтэрнэтам"

#: src/octoprint/templates/navbar/settings.jinja2:1
msgid "Settings"
msgstr "Налады"

#: src/octoprint/templates/overlays/dragndrop.jinja2:5
msgid "Upload locally"
msgstr "Загрузіць лакальна"

#: src/octoprint/templates/overlays/dragndrop.jinja2:7
#: src/octoprint/templates/sidebar/files.jinja2:75
msgid "Upload to SD"
msgstr "Загрузіць на SD-картку"

#: src/octoprint/templates/overlays/dragndrop.jinja2:7
msgid "Cannot upload to printer's SD while printing"
